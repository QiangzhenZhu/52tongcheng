    <declare-styleable name="BubbleImageView">
        <attr name="bubble_arrowTop" format="dimension|reference" />
        <attr name="bubble_arrowWidth" format="dimension|reference" />
        <attr name="bubble_arrowHeight" format="dimension|reference" />
        <attr name="bubble_arrowOffset" format="dimension|reference" />
        <attr name="bubble_angle" format="dimension|reference" />
        <attr name="bubble_arrowLocation" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
        <attr name="bubble_showText" format="boolean" />
        <attr name="bubble_showShadow" format="boolean" />
    </declare-styleable>
    <declare-styleable name="QPhotoView">
        <attr name="showCrop" format="boolean"/>
        <attr name="sendBtnBackground" format="reference"/>
        <attr name="albumTextColor" format="color"/>
        <attr name="cropEnabledTextColor" format="color"/>
        <attr name="cropNoUnEnableTextColor" format="color"/>
        <attr name="sendEnabledTextColor" format="color"/>
        <attr name="sendNoUnEnableTextColor" format="color"/>
        <attr name="maxSeleteCount" format="integer"/>
    </declare-styleable>
    <declare-styleable name="BubbleView">
        <attr name="arrowWidth" format="dimension" />
        <attr name="angle" format="dimension" />
        <attr name="arrowHeight" format="dimension" />
        <attr name="arrowPosition" format="dimension" />
        <attr name="bubbleColor" format="color" />
        <attr name="arrowCenter" format="boolean" />
        <attr name="arrowLocation" format="enum">
            <enum name="left" value="0x00" />
            <enum name="right" value="0x01" />
            <enum name="top" value="0x02" />
            <enum name="bottom" value="0x03" />
        </attr>
    </declare-styleable>
    <declare-styleable name="BezierView">
        <attr name="paint_color" format="color"/>
        <attr name="paint_width" format="dimension"/>
        <attr name="height_proportion" format="fraction"/>
    </declare-styleable>
    <declare-styleable name="ChatItemView">
        <attr name="show_nickname">
            <enum name="noShowEvery" value="0"/>
            <enum name="showLeft" value="1"/>
            <enum name="showRight" value="2"/>
            <enum name="showEvery" value="3"/>
        </attr>
        <attr name="LeftPhotoSize" format="dimension"/>
        <attr name="RightPhotoSize" format="dimension"/>
        <attr name="DefaultLeftChatTextSize" format="dimension"/>
        <attr name="DefaultRightChatTextSize" format="dimension"/>
        <attr name="DefaultNicknameTextSizeLeft" format="dimension"/>
        <attr name="DefaultNicknameTextSizeRight" format="dimension"/>
        <attr name="DefaultBackgroundRight" format="color"/>
        <attr name="DefaultBackgroundLeft" format="color"/>
        <attr name="DefaultNicknameTextColorLeft" format="color"/>
        <attr name="DefaultNicknameTextColorRight" format="color"/>
        <attr name="DefaultChatTextColorLeft" format="color"/>
        <attr name="DefaultChatTextColorRight" format="color"/>
    </declare-styleable>
</resources>